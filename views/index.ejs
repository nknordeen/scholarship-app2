<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <div id="page">
      <header>
        <h1>findMonies Scholarships</h1>
      </header>
      <div id="table">
        <table>
          <tr id="tableHead">
            <th>Name</th>
            <th>Amount</th>
            <th>Description</th>
          </tr>
          <% scholarships.forEach(function(item){ %>
            <tr class="tableBody">
              <td><%=item.name %></td>
              <td><%=item.amount %></td>
              <td><%=item.description %></td>
            </tr>
          <% }); %>
        </table>
      </div>
    </div>
  </body>
</html>
